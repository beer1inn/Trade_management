generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Trades {
  id        Int      @id @default(autoincrement())
  name      String
  ticker    String   
  open      Float
  high      Float
  low       Float
  last      Float
  settle    Float
  change    Float
  estVolume Int
  market    String
  timestamp      DateTime @default(now()) // Date/time of the trade
}
